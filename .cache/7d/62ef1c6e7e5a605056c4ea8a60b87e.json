-Light\";\r\n    src: url('../fonts/Inter-Light.ttf');\r\n}\r\n@font-face {\r\n    font-family: \"Inter-Medium\";\r\n    src: url('../fonts/Inter-Medium.ttf');\r\n}\r\n@font-face {\r\n    font-family: \"Inter-Regular\";\r\n    src: url('../fonts/Inter-Regular.ttf');\r\n}\r\n@font-face {\r\n    font-family: \"Inter-Bold\";\r\n    src: url('../fonts/Inter-Bold.ttf');\r\n}\r\n\r\n//COLORS\r\n$dark-bg: hsl(230, 17%, 14%);\r\n$text-white: hsl(0, 0%, 100%);\r\n$text-gray-blue: hsl(228, 12%, 44%);\r\n$text-blue: hsl(228, 34%, 66%);\r\n$card-bg: hsl(228, 28%, 20%);\r\n$border-light-blue: hsl(208%, 92%, 53%);\r\n$lime-green: hsl(163, 72%, 41%);\r\n\r\n* {\r\n    margin: 0;\r\n    padding: 0;\r\n    box-sizing: border-box;\r\n}\r\nbody {\r\n    background-color: $dark-bg;\r\n}\r\n\r\n//IMPORTS\r\n@import './_nav';\r\n@import './_boxes';\r\n\r\n","styles\\__nav.scss":".nav {\r\n    max-width: 1440px;\r\n    margin: auto;\r\n    display: flex;\r\n    justify-content: space-between;\r\n    align-items: center;\r\n    padding: 25px 10px;\r\n    &__left-side {\r\n        h1 {\r\n            color: $text-white;\r\n            padding-bottom: 5px;\r\n            font-family: \"Inter-Medium\";\r\n        }\r\n        p {\r\n            color: $text-gray-blue;\r\n            font-family: \"Inter-Regular\";\r\n            font-size: 0.75rem;\r\n            letter-spacing: 0.0125rem;\r\n            font-weight: 700;\r\n        }\r\n    }\r\n    &__right-side {\r\n        display: flex;\r\n        align-items: center;\r\n        p {\r\n            font-family: \"Inter-Medium\";\r\n            color: $text-gray-blue;\r\n            font-weight: 700;\r\n            font-size: 0.875rem;\r\n            margin-right: 15px;\r\n        }\r\n        .toggle {\r\n            width: 40px;\r\n            height: 20px;\r\n            border-radius: 16px;\r\n            cursor: pointer;\r\n        }\r\n        .toggle__input {\r\n            display: none;\r\n        }\r\n        .toggle__fill {\r\n            position: relative;\r\n            width: 40px;\r\n            height: 20px;\r\n            border-radius: 16px;\r\n            background: linear-gradient(to right,hsl(210, 78%, 56%), hsl(146, 68%, 55%));\r\n        }\r\n        .toggle__fill::after {\r\n            content: \"\";\r\n            position: absolute;\r\n            top: 3px;\r\n            left: 2.5px;\r\n            height: 15px;\r\n            width: 15px;\r\n            background-color: $dark-bg;\r\n            border-radius: 20px;\r\n            transition: transform 300ms;\r\n        }\r\n        .toggle__input:checked + .toggle__fill::after {\r\n            transform: translateX(20px);\r\n        }   \r\n    }\r\n}","styles\\__boxes.scss":"@mixin flexCenter {\r\n    display: flex;\r\n    justify-content: center;\r\n    align-items: center;\r\n}\r\n.box {\r\n    width: 250px;\r\n    height: 220px;\r\n    background-color: $card-bg;\r\n    position: relative;\r\n    margin: 10px;\r\n    border-bottom-left-radius: 10px;\r\n    border-bottom-right-radius: 10px;\r\n    &__color-bar {\r\n        position: absolute;\r\n        top: 0;\r\n        left: 0;\r\n        width: 100%;\r\n        height: 4px;\r\n        background-color: $border-light-blue;\r\n        border-top-left-radius: 10px;\r\n        border-top-right-radius: 10px;\r\n    }\r\n    &__social {\r\n        @include flexCenter();\r\n        padding-top: 30px;\r\n        padding-bottom: 25px;\r\n        p {\r\n            color: $text-blue;\r\n            font-family: \"Inter-Regular\";\r\n            font-weight: 700;\r\n            font-size: 0.75rem;\r\n        }\r\n    }\r\n    .fa-facebook-square {\r\n        color: hsl(207, 89, 52);\r\n        margin-right: 8px;\r\n    }\r\n    &__count {\r\n        @include flexCenter();\r\n        flex-direction: column;\r\n        padding-bottom: 25px;\r\n        h1 {\r\n            font-size: 3.5rem;\r\n            color: $text-white;\r\n            font-family: \"Inter-Medium\";\r\n        }\r\n        p {\r\n            color: $text-blue;\r\n            font-family: \"Inter-Regular\";\r\n            font-size: 0.75rem;\r\n            letter-spacing: 6px;\r\n        }\r\n    }\r\n    &__updated-count {\r\n        @include flexCenter();\r\n        .fa-angle-up {\r\n            color: $lime-green;\r\n            margin-right:4px;\r\n        }\r\n        p {\r\n            color: $lime-green;\r\n            font-size: 0.75rem;\r\n            font-family: \"Inter-Regular\";\r\n            font-weight: 700;\r\n        }\r\n    }\r\n}"},"lineCount":null}},"error":null,"hash":"8241ea860fb6697dc24a4452c3629e31","cacheData":{"env":{}}}